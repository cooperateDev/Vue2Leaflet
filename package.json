{
  "name": "vue2-leaflet",
  "version": "2.0.0-beta.5",
  "description": "Vue2 leaflet library",
  "author": "Mickael Bouchaud",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "bili --env.BUILD production",
    "prepare": "npm run build",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "docs:serve": "docsify serve ./docs -p 4000 -o"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^8.2.6",
    "bili": "^4.1.0",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "leaflet": "^1.4.0",
    "lint-staged": "^7.2.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-vue": "^4.6.1",
    "typescript": "^3.1.6",
    "vue": "^2.5.22",
    "vue-template-compiler": "^2.5.22"
  },
  "files": [
    "dist/",
    "types/*.d.ts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "vue",
    "leaflet"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ],
    "*.vue": [
      "eslint",
      "git add"
    ]
  },
  "main": "dist/vue2-leaflet.cjs.js",
  "unpkg": "dist/vue2-leaflet.umd.min.js",
  "module": "dist/vue2-leaflet.esm.js",
  "sideEffects": false,
  "peerDependencies": {
    "@types/leaflet": "^1.2.11",
    "leaflet": "^1.3.4",
    "vue": "^2.5.17"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/KoRiGaN/Vue2Leaflet.git"
  },
  "typings": "types/index.d.ts"
}
