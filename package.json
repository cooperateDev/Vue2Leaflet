{
  "name": "vue2-leaflet",
  "version": "2.0.0-beta.0",
  "description": "Vue2 leaflet library",
  "author": "Mickael Bouchaud",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --entry src/main.js",
    "lint": "vue-cli-service lint",
    "docs:serve": "docsify serve ./docs -p 4000 -o",
    "test:types": "tsc -p ./types/test/tsconfig.json",
    "test:unit": "vue-cli-service test:unit"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "lint-staged": "^7.2.2",
    "typescript": "^3.1.6",
    "vue": "^2.5.17",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0",
    "vue-template-compiler": "^2.5.17"
  },
  "files": [
    "dist/",
    "types/*.d.ts"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "vue",
    "leaflet"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "main": "dist/vue2-leaflet.umd.min.js",
  "module": "dist/vue2-leaflet.common.js",
  "peerDependencies": {
    "@types/leaflet": "^1.2.11",
    "vue": "^2.5.17",
    "leaflet": "^1.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/KoRiGaN/Vue2Leaflet.git"
  },
  "typings": "types/index.d.ts",
  "unpkg": "dist/vue2-leaflet.umd.min.js"
}
